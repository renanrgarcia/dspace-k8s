{
  "name": "dspace-angular",
  "version": "9.0.0",
  "description": "DSpace Frontend - Angular Universal application",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4000",
    "start:dev": "ng serve --configuration development --host 0.0.0.0 --port 4000",
    "start:prod": "ng serve --configuration production --host 0.0.0.0 --port 4000",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:ssr": "ng build && ng run dspace-angular:server",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr": "node dist/server/main.js",
    "sync-i18n": "ng run dspace-angular:sync-i18n"
  },
